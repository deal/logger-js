{
  "compilerOptions": {
    "lib": ["es5", "es6", "es7"],
    "baseUrl": ".",
    "moduleResolution": "node",
    "module": "commonjs",
    "target": "es5",
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "strict": true,
    "allowSyntheticDefaultImports": true,
    "resolveJsonModule": true,
    "esModuleInterop": true
  },
  "exclude": ["../node_modules"]
}
